# Database
DATABASE_PATH=./data/linkedin-jobs.db

# Scraping
MAX_CONCURRENT_REQUESTS=2
REQUEST_DELAY_MIN=3000
REQUEST_DELAY_MAX=7000
MAX_PAGES_PER_SEARCH=10
MAX_RETRIES=3

# Proxy (optional)
PROXY_ENABLED=false
PROXY_HOST=
PROXY_PORT=
PROXY_USERNAME=
PROXY_PASSWORD=

# Logging
LOG_LEVEL=info
LOG_FILE=./logs/scraper.log

# Browser
HEADLESS=true
BROWSER_TYPE=chromium
